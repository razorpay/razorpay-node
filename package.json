{
  "name": "razorpay",
  "version": "2.0.6",
  "description": "Official Node SDK for Razorpay API",
  "main": "dist/razorpay",
  "typings": "dist/razorpay",
  "scripts": {
    "prepublish": "npm test",
    "clean": "rm -rf dist && mkdir dist",
    "cp-ts": "cp lib/razorpay.d.ts dist/",
    "build:commonjs": "tsc",
    "build": "npm run clean && npm run build:commonjs && npm run cp-ts",
    "debug": "npm run build && node-debug examples/index.js",
    "test": "npm run build && mocha --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/razorpay/razorpay-node.git"
  },
  "keywords": [
    "razorpay",
    "payments",
    "node",
    "nodejs",
    "razorpay-node"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "chai": "^4.1.2",
    "deep-equal": "^1.0.1",
    "mocha": "^5.2.0",
    "nock": "^9.3.3",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "qs": "^6.9.3"
  }
}
